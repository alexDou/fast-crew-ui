{
  "name": "poets-crew-ai",
  "version": "16.0.0",
  "private": true,
  "description": "Poets crew UI",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build --webpack && open .next/analyze/nodejs.html",
    "clean": "rimraf .next",
    "check": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "knip": "knip",
    "lint": "eslint . --ext js,ts,tsx --cache",
    "lint:fix": "eslint . --ext js,ts,tsx --fix --cache && biome format --write .",
    "lint:ci": "eslint . --ext js,ts,tsx --max-warnings=0 && biome format .",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "biome format --write"
    ],
    "*.{css,md,json}": [
      "biome format --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next/bundle-analyzer": "^16.0.1",
    "@next/third-parties": "^16.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "js-cookie": "^3.0.5",
    "ky": "^1.14.3",
    "lucide-react": "^0.548.0",
    "next": "^16.0.7",
    "next-intl": "^4.4.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.65.0",
    "react-wrap-balancer": "^1.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "usehooks-ts": "^3.1.1",
    "webpack": "^5.102.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.0.0",
    "@playwright/test": "^1.58.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "1.0.0",
    "commitizen": "^4.3.0",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "knip": "^5.66.4",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.47",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "bugs": {
    "url": ""
  },
  "license": "MIT",
  "keywords": [
    "Ai image processing",
    "AI image",
    "crewai"
  ],
  "packageManager": "yarn@4.12.0"
}
